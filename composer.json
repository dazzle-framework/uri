{
    "name": "dazzlesoftware/uri",
    "type": "library",
    "description": "Dazzle Uri Package",
    "keywords": ["dazzle", "framework", "uri"],
    "homepage": "https://github.com/dazzle-framework/uri",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0",
